<%= form_with model: @user, url: users_profile_update_path do |f| %>
  <div>
    <%= f.label :avater %>
    <%= f.file_field :avater %>
  </div>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :profile %>
    <%= f.text_area :profile %>
  </div>

  <%= f.submit "登録", class: "btn btn-primary" %>
<% end %> 